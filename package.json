{
  "name": "microflo-luftdaten",
  "version": "0.1.0",
  "description": "MicroFlo firmware for luftdaten.info air quality sensors",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "microflo generate --components ./node_modules/microflo-core/components --target=arduino graphs/blink.fbp build/blink.ino",
    "start": "microflo runtime --wait-connect 3 --graph graphs/blink.fbp --componentmap build/blink.component.map.json",
    "build": "`node build.js`"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c-base/microflo-luftdaten.git"
  },
  "keywords": [
    "microflo",
    "arduino",
    "fbp",
    "dataflow"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/c-base/microflo-luftdaten/issues"
  },
  "homepage": "https://luftdaten.info/",
  "devDependencies": {
    "microflo": "^0.6.0",
    "microflo-core": "^0.6.0"
  }
}
